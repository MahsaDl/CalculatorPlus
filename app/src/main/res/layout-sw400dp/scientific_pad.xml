<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/firstRow"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/inv"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="lNV"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/angleType"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/angleType"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="@string/deg"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/sin"
            app:layout_constraintStart_toEndOf="@id/inv"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText"

            />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/sin"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="sin"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/cos"
            app:layout_constraintStart_toEndOf="@id/angleType"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/cos"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="cos"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/tan"
            app:layout_constraintStart_toEndOf="@id/sin"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/tan"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"

            android:layout_height="match_parent"
            android:text="tan"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/cos"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/secondRow"
        android:layout_width="match_parent"
        android:layout_height="56dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/exponential"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="e"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/naturalLog"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/naturalLog"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="ln"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/exponential"
            app:layout_constraintStart_toEndOf="@id/log"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/log"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="log"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/factorial"
            app:layout_constraintStart_toEndOf="@id/naturalLog"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/factorial"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="!"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toStartOf="@id/power"
            app:layout_constraintStart_toEndOf="@id/log"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/power"
            style="@style/Widget.CalculatorPlus.NumPad.Secondary"
            android:layout_width="56dp"
            android:layout_height="match_parent"
            android:text="^"
            android:textColor="#6750A4"
            android:textAppearance="@style/TextAppearance.CalculatorPlus.Headline5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/factorial"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>